@article{farinelli2017,
  title = {Interacting with Team Oriented Plans in Multi-Robot Systems},
  author = {Farinelli, Alessandro and Raeissi, Masoume M. and Marchi, Nicolo' and Brooks, Nathan and Scerri, Paul},
  year = {2017},
