  langid = {english},
  keywords = {evolution strategies,nature-inspired algorithm,novel algorithm,particle swarm optimization,swarm intelligence},
  file = {/home/danzi/Zotero/storage/6QB487Y6/Camacho-Villalón et al. - 2023 - Exposing the grey wolf, moth-flame, whale, firefly.pdf;/home/danzi/Zotero/storage/6H8KHCFQ/itor.html}
